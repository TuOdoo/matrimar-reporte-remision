<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="wizard_stock_picking_form" model="ir.ui.view">
        <field name="name">wizard.stock.picking.form</field>
        <field name="model">wizard.stock.picking</field>
        <field name="arch" type="xml">
            <form string="">
                <header></header>
                <sheet>
                    <group>
                        <group>
                            <field name="fecha_inicio" />
                        </group>
                        <group>
                            <field name="fecha_fin" />
                        </group>
                    </group>
                    <group>
                        <group>
                            <field name="customer_id"/>
                            <field name="partner_id" />
                        </group>
                    </group>
                   
                    <footer>
                        <button string="Filtrar"
                            name="generar_reporte"
                            type="object"
                            class="oe_highlight" />

                        <button string="Cerrar"
                            name="cerrar"
                            type="object"
                            class="btn-danger btn" />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>